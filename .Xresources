URxvt.font: xft:Hack:size=15
!! URxvt*font: xft:PowerlineSymbols for Powerline:size=10
!! URxvt.font: xft:Terminus:pixelsize=12,xft:Inconsolata\ for\ Powerline:pixelsize=12

URxvt.transparent: true
URxvt.shading: 20

URxvt.scrollBar: false

URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,matcher,resize-font
URxvt.url-select.launcher:  chromium
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

URxvt.resize-font.smaller:	C--
URxvt.resize-font.bigger:	C-+

